{{>header_for_modifications}}

program {{programName}}App;

uses
  Vcl.Forms,
  FormMain in 'FormMain.pas' {MainForm},{{#models}}{{#model}}
  {{classFilename}} in 'models\{{classFilename}}.pas',{{/model}}{{/models}}{{#apiInfo}}{{#apis}}{{#operations}}
  {{classname}}Interface in 'apis\{{classname}}Interface.pas',
  {{classname}}.Rest in 'apis\{{classname}}.Rest.pas',
  {{classname}} in 'apis\{{classname}}.pas'{{^-last}}, {{/-last}}{{/operations}}{{/apis}}{{/apiInfo}},
  {{programName}}Server
,Model.ExtInfo in 'models\Model.ExtInfo.pas'
,Model.UNKNOWN_BASE_TYPE in 'models\Model.UNKNOWN_BASE_TYPE.pas';

{$R *.res}

begin
  ReportMemoryLeaksOnShutdown := true;
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TMainForm, MainForm);
  Application.Run;
end.
